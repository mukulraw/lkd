<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrtecks.loankarado">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Bean"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".OTP"></activity>
        <activity android:name=".Login" />
        <activity android:name=".PropertySelf" />
        <activity android:name=".HomeSelf" />
        <activity android:name=".PropertySalaried" />
        <activity android:name=".HomeSalaried" />
        <activity android:name=".Business" />
        <activity
            android:name=".Personal"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>